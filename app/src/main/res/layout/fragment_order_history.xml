                android:textColor="#3B464C"
                app:layout_constraintBaseline_toBaselineOf="@+id/textView78"
                app:layout_constraintStart_toEndOf="@+id/textView78" />

            <TextView
                android:id="@+id/textView85"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="Отправлено"
                android:textColor="#3B464C"
                app:layout_constraintBaseline_toBaselineOf="@+id/textView79"
                app:layout_constraintStart_toEndOf="@+id/textView79" />

            <TextView
                android:id="@+id/textView86"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="328277522"
                android:textColor="#3B464C"
                app:layout_constraintBaseline_toBaselineOf="@+id/textView80"
                app:layout_constraintStart_toEndOf="@+id/textView80" />

            <TextView
                android:id="@+id/textView87"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/montserrat_bold"
                android:text="Состав заказа"
                android:textColor="#FF9B39"
